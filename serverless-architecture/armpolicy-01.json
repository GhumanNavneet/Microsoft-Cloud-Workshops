        },
        {
          "allOf": [
            {
              "field": "type",
              "equals": "Microsoft.DevTestLab/labs"
            }

          ]
        },
        {
          "allOf": [
            {
              "field": "type",
              "equals": "Microsoft.Compute/virtualMachines"
